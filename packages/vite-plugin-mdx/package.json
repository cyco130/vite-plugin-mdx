{
  "name": "@cyco130/vite-plugin-mdx",
  "version": "2.1.7",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint --strict"
  },
  "description": "Vite plugin for using MDX",
  "license": "MIT",
  "repository": "github:cyco130/vite-plugin-mdx",
  "keywords": [
    "vite",
    "mdx"
  ],
  "peerDependencies": {
    "vite": "3 || 4 || 5 || 6"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^6.0.2",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "publint": "^0.3.12",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "source-map": "^0.7.6",
    "vfile": "^6.0.3"
  }
}
