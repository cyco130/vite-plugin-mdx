{
  "name": "@cyco130/vite-plugin-mdx",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "description": "Vite plugin for using MDX",
  "license": "MIT",
  "repository": "https://github.com/cyco130/vite-plugin-mdx",
  "keywords": [
    "vite",
    "mdx"
  ],
  "peerDependencies": {
    "vite": "3 || 4"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^2.1.2",
    "@types/node": "^18.11.17",
    "eslint": "^8.30.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.2.1",
    "@rollup/pluginutils": "^5.0.2",
    "source-map": "^0.7.4",
    "vfile": "^5.3.6"
  }
}
